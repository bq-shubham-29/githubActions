name: helloWorldDockerImage

on: 
  workflow_dispatch:
  push:
    branches:
      - main
jobs:
  hello-world:
    runs-on: self-hosted
 
    steps:
      - name: checkout
        uses: actions/checkout@v2

      - name: pull docker hello-world image
        run: |-
          echo ""
          sleep 600
          echo ""
